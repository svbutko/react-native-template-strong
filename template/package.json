{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package && react-native setup-ios-permissions",
    "postversion": "react-native-version",
    "open:xcode": "open ios/HelloWorld.xcworkspace",
    "open:android-studio": "studio android",
    "ts": "tsc -p .",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-reset-cache": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "android:run": "node node_modules/react-native/local-cli/cli.js run-android --no-packager",
    "android:run+start": "node node_modules/react-native/local-cli/cli.js run-android --no-packager && node node_modules/react-native/local-cli/cli.js start",
    "android:build-release-bundle-nix": "(cd android/ && ./gradlew bundleRelease)",
    "android:build-release-apk-nix": "(cd android/ && ./gradlew assembleRelease)",
    "android:clean-nix": "(cd android/ && ./gradlew clean)",
    "android:build-release-bundle-win": "cd android && gradlew bundleRelease",
    "android:build-release-apk-win": "cd android && gradlew assembleRelease",
    "android:clean-win": "cd android && gradlew clean",
    "android:bundle": "node node_modules/react-native/local-cli/cli.js bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/ --dev false --sourcemap-sources-root=build",
    "android:bundle-visualizer": "react-native-bundle-visualizer --platform android",
    "ios:run": "node node_modules/react-native/local-cli/cli.js run-ios --no-packager",
    "ios:bundle-visualizer": "react-native-bundle-visualizer --platform ios",
    "check-dependencies-updates": "npm outdated",
    "check-pods-updates": "cd ios && pod outdated",
    "environment:development-nix": "ENVFILE=.env",
    "environment:test-nix": "ENVFILE=.env.test",
    "environment:staging-nix": "ENVFILE=.env.staging",
    "environment:production-nix": "ENVFILE=.env.production",
    "environment:development-win": "SET ENVFILE=.env",
    "environment:test-win": "SET ENVFILE=.env.test",
    "environment:staging-win": "SET ENVFILE=.env.staging",
    "environment:production-win": "SET ENVFILE=.env.production",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "tests": "jest",
    "tests:watch": "jest --watchAll",
    "tests:coverage": "jest --coverage",
    "e2e:build-android-debug": "npx detox build --configuration android.emu.debug",
    "e2e:android-debug": "npx detox test --configuration android.emu.debug",
    "e2e:build-android-release": "npx detox build --configuration android.emu.release",
    "e2e:android-release": "npx detox test --configuration android.emu.release",
    "e2e:build-ios-debug": "npx detox build --configuration ios.emu.debug",
    "e2e:ios-debug": "npx detox test --configuration ios.emu.debug",
    "e2e:build-ios-release": "npx detox build --configuration ios.emu.release",
    "e2e:ios-release": "npx detox test --configuration ios.emu.release",
    "eslint:run": "eslint --ext=jsx,ts,tsx",
    "eslint:fix": "eslint --fix --ext=jsx,ts,tsx .",
    "generate-image-resources": "img-res-gen --dir=resources/images --out=src/common/ImageResources.g.ts --ts",
    "prepare": "husky install",
    "setup-permissions": "npx react-native setup-ios-permissions"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.1",
    "@react-native-community/datetimepicker": "7.0.1",
    "@react-native-community/netinfo": "9.3.9",
    "@react-native-menu/menu": "0.7.3",
    "@reduxjs/toolkit": "1.9.3",
    "dayjs": "1.11.7",
    "intl": "1.2.5",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-config": "1.5.0",
    "react-native-device-info": "10.6.0",
    "react-native-gesture-handler": "2.9.0",
    "react-native-image-crop-picker": "0.39.0",
    "react-native-in-app-review": "4.3.3",
    "react-native-localization": "2.3.1",
    "react-native-mask-input": "1.2.3",
    "react-native-navigation": "7.32.1",
    "react-native-navigation-hooks": "6.3.0",
    "react-native-orientation-locker": "1.5.0",
    "react-native-permissions": "3.8.0",
    "react-native-reanimated": "3.0.2",
    "react-native-reanimated-carousel": "3.3.0",
    "react-native-share": "8.2.1",
    "react-native-size-matters": "0.4.0",
    "react-native-sfsymbols": "1.2.1",
    "react-native-vector-icons": "9.2.0",
    "react-redux": "8.0.5",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/runtime": "7.21.0",
    "@types/intl": "1.2.0",
    "@types/jest": "29.5.0",
    "@types/lodash": "4.14.192",
    "@types/react": "18.0.34",
    "@types/react-redux": "7.1.25",
    "@types/react-test-renderer": "18.0.0",
    "@types/react-native-vector-icons": "6.4.13",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "babel-jest": "29.5.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "detox": "20.7.0",
    "eslint": "8.38.0",
    "eslint-config-react-strong": "2.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.1",
    "metro-react-native-babel-preset": "0.76.1",
    "patch-package": "6.5.1",
    "prettier": "2.8.7",
    "react-native-bundle-visualizer": "3.1.3",
    "react-native-config-node": "0.0.3",
    "react-native-dev-menu": "4.1.1",
    "react-native-image-resource-generator": "1.0.2",
    "react-native-version": "4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "engines": {
    "npm": ">= 6.14.18",
    "yarn": "Please use npm >= 6.14.18"
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "PhotoLibrary"
  ]
}
