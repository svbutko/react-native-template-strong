{
  "name": "HelloWorld",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package && react-native setup-ios-permissions",
    "postversion": "react-native-version",
    "open:xcode": "open ios/HelloWorld.xcworkspace",
    "open:android-studio": "studio android",
    "prestart": "sb-rn-get-stories",
    "storybook:watcher": "sb-rn-watcher",
    "storybook:generate-stories": "sb-rn-get-stories",
    "ts": "tsc -p .",
    "start": "npx react-native start",
    "start-reset-cache": "npx react-native start -- --reset-cache",
    "android:run": "npx react-native run-android --no-packager",
    "android:run+start": "npx react-native run-android --no-packager && npx react-native start",
    "android:build-release-bundle-nix": "(cd android/ && ./gradlew bundleRelease)",
    "android:build-release-apk-nix": "(cd android/ && ./gradlew assembleRelease)",
    "android:clean-nix": "(cd android/ && ./gradlew clean)",
    "android:build-release-bundle-win": "cd android && gradlew bundleRelease",
    "android:build-release-apk-win": "cd android && gradlew assembleRelease",
    "android:clean-win": "cd android && gradlew clean",
    "android:bundle": "npx react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/ --dev false --sourcemap-sources-root=build",
    "android:bundle-visualizer": "react-native-bundle-visualizer --platform android",
    "ios:run": "npx react-native run-ios --no-packager",
    "ios:bundle-visualizer": "react-native-bundle-visualizer --platform ios",
    "ios:bundle-install": "cd ios && bundle install",
    "ios:bundle-pod-install": "cd ios && bundle exec pod install",
    "ios:bundle-pod-install-repo-update": "cd ios && bundle exec pod install --repo-update",
    "ios:bundle-check-pods-updates": "cd ios && bundle exec pod outdated",
    "environment:development-nix": "ENVFILE=.env",
    "environment:test-nix": "ENVFILE=.env.test",
    "environment:staging-nix": "ENVFILE=.env.staging",
    "environment:production-nix": "ENVFILE=.env.production",
    "environment:development-win": "SET ENVFILE=.env",
    "environment:test-win": "SET ENVFILE=.env.test",
    "environment:staging-win": "SET ENVFILE=.env.staging",
    "environment:production-win": "SET ENVFILE=.env.production",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "tests": "jest",
    "tests:watch": "jest --watchAll",
    "tests:coverage": "jest --coverage",
    "e2e:build-android-debug": "npx detox build --configuration android.emu.debug",
    "e2e:android-debug": "npx detox test --configuration android.emu.debug",
    "e2e:build-android-release": "npx detox build --configuration android.emu.release",
    "e2e:android-release": "npx detox test --configuration android.emu.release",
    "e2e:build-ios-debug": "npx detox build --configuration ios.sim.debug",
    "e2e:ios-debug": "npx detox test --configuration ios.sim.debug",
    "e2e:build-ios-release": "npx detox build --configuration ios.sim.release",
    "e2e:ios-release": "npx detox test --configuration ios.sim.release",
    "eslint:run": "eslint --ext=jsx,ts,tsx",
    "eslint:fix": "eslint --fix --ext=jsx,ts,tsx .",
    "generate-image-resources": "img-res-gen --dir=resources/images --out=src/common/ImageResources.g.ts --ts",
    "prepare": "husky install",
    "prettier": "prettier 'src/**/*.ts'",
    "setup-permissions": "npx react-native setup-ios-permissions",
    "npm-install-force": "npm i --f",
    "check-dependencies-updates": "npm outdated"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.19.3",
    "@react-native-community/datetimepicker": "7.4.2",
    "@react-native-community/netinfo": "9.4.1",
    "@react-native-menu/menu": "0.8.0",
    "@reduxjs/toolkit": "1.9.5",
    "@shopify/flash-list": "1.5.0",
    "dayjs": "1.11.9",
    "intl": "1.2.5",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-config": "1.5.1",
    "react-native-device-info": "10.8.0",
    "react-native-gesture-handler": "2.12.1",
    "react-native-image-crop-picker": "0.40.0",
    "react-native-in-app-review": "4.3.3",
    "react-native-localization": "2.3.2",
    "react-native-mask-input": "1.2.3",
    "react-native-navigation": "7.37.0",
    "react-native-navigation-hooks": "6.3.0",
    "react-native-orientation-locker": "1.5.0",
    "react-native-permissions": "3.8.4",
    "react-native-reanimated": "3.4.2",
    "react-native-reanimated-carousel": "3.5.1",
    "react-native-share": "9.2.3",
    "react-native-size-matters": "0.4.0",
    "react-native-sfsymbols": "1.2.1",
    "react-native-vector-icons": "10.0.0",
    "react-redux": "8.1.2",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@babel/preset-env": "7.22.10",
    "@babel/runtime": "7.22.11",
    "@react-native-community/slider": "4.4.3",
    "@react-native/metro-config": "0.72.11",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-controls": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-ondevice-actions": "6.5.6",
    "@storybook/addon-ondevice-knobs": "6.5.6",
    "@storybook/addon-ondevice-controls": "6.5.6",
    "@storybook/addon-ondevice-notes": "6.5.6",
    "@storybook/core-common": "6.5.16",
    "@storybook/react-native": "6.5.6",
    "@types/intl": "1.2.0",
    "@types/jest": "29.5.4",
    "@types/lodash": "4.14.197",
    "@types/react": "18.2.21",
    "@types/react-redux": "7.1.26",
    "@types/react-test-renderer": "18.0.0",
    "@types/react-native-vector-icons": "6.4.14",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "babel-jest": "29.6.4",
    "babel-plugin-transform-remove-console": "6.9.4",
    "detox": "20.11.4",
    "eslint": "8.48.0",
    "eslint-config-react-strong": "2.2.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "lint-staged": "14.0.1",
    "metro-react-native-babel-preset": "0.76.8",
    "patch-package": "8.0.0",
    "prettier": "3.0.2",
    "react-dom": "18.2.0",
    "react-native-bundle-visualizer": "3.1.3",
    "react-native-config-node": "0.0.3",
    "react-native-dev-menu": "4.1.1",
    "react-native-image-resource-generator": "1.0.2",
    "react-native-safe-area-context": "4.7.1",
    "react-native-version": "4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16",
    "npm": ">= 6.14.18",
    "yarn": "Please use npm >= 6.14.18"
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "PhotoLibrary"
  ]
}
